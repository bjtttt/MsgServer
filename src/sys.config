[
  %% write log files
  {sasl, [ 
         %% shell error logging 
         %{sasl_error_logger, "/tmp/log/msapp.sasl_log"}, 
         %% only report errors 
         {errlog_type, error}, 
         %% define the parameters of the rotating log 
         %% the log file directory 
         {error_logger_mf_dir,"/tmp/log"}, 
         %% # bytes per logfile 
         {error_logger_mf_maxbytes,10485760}, % 10 MB 
         %% maximum number of 
         {error_logger_mf_maxfiles, 100} 
        ]},
  {msapp,[]}
].
